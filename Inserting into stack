#include <iostream>
#include <stack>

using namespace std;

int main() {
    stack<int> s;

    // Pushing elements into the stack
    s.push(5);
    s.push(10);
    s.push(15);

    // Displaying elements of the stack
    cout << "Stack elements:";
    while (!s.empty()) {
        cout << ' ' << s.top();
        s.pop();
    }
    cout << endl;

    return 0;
}
